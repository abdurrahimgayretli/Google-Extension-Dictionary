{
    "manifest_version": 2,
    "name": "Dictionary",
    "version": "1.0",
    "description": "Dictionary Notifiaction",
    "browser_action": {
        "default_popup": "popup.html"
    },

    "background":{
        "scripts":["eventPage.js"],
        "persistent": false
    },

    "permissions": [
        "storage",
        "notifications",
        "contextMenus",
        "alarms"
    ]

}